{
  "IncludeAspNetIdentity": false,
  "EnableCors": true,
  "DIContainer": "Autofac", /* LightInject DryIoc Autofac*/
  "LoggingProvider": "Log4Net", /* Log4Net, Serilog */
  "EnableScheduler": true,
  "EnableAntiforgery": true,
  "Scheduler": "Hangfire",
  "AppAssemblyPrefixes": "Copious",
  "EnableDocument": true,
  "AuthenticationType": "None", /* None, Jwt */
  "DocumentDb": "Postgres",
  "Mapper" :  "Mapster" , /* Mapster, Automapper*/
  "DocumentDbConnection": "PostgreSqlProvider", /* provide the connection string key here */
  "DefaultDocumentStorageProvider": "FileSystem",
  "DefaultDocumentStorageProviderOptions": [
    {
      "Key": "BasePath",
      "Value": "D: \\Buffer"
    }
  ]
}